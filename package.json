{
  "name": "validator-auction-backend-server",
  "version": "0.7.4",
  "description": "Trustlines Validator Auction Backend Server",
  "main": "api.js",
  "engines": {
    "node": ">=8.12.0"
  },
  "scripts": {
    "start": "node -r esm -r dotenv/config ./api/index.js",
    "dev": "NODE_ENV=development BUNYAN_LOGLEVEL=10 nodemon -r esm -r dotenv/config -i ./logs/ ./api/index.js | bunyan -L",
    "dev-blocking": "NODE_ENV=development nodemon $NODE_DEBUG_OPTION -r esm -r dotenv/config -i ./logs/ ./api/index.js | bunyan -L",
    "watch-server": "NODE_ENV=development BUNYAN_LOGLEVEL=10 nodemon -r esm -r dotenv/config -i ./logs/ | bunyan -L",
    "build": "npm run build-js && npm run build-css",
    "lint": "eslint ./api/**/*.js ./lib/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/eth.events/api.git"
  },
  "keywords": [
    "ethereum",
    "api",
    "eth.events",
    "redis",
    "postgres",
    "node",
    "passport",
    "express"
  ],
  "author": "eth.events <team@eth.events>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://gitlab.com/eth.events/api/issues"
  },
  "homepage": "https://gitlab.com/eth.events/api#readme",
  "devDependencies": {
    "@babel/core": "7.1.2",
    "@babel/preset-env": "7.1.0",
    "babel-loader": "8.0.4",
    "concurrently": "4.0.1",
    "errorhandler": "1.5.0",
    "eslint": "5.6.1",
    "nodemon": "1.18.4",
    "response-time": "2.3.2"
  },
  "dependencies": {
    "async": "2.6.2",
    "axios": "0.18.0",
    "body-parser": "1.18.3",
    "bunyan": "1.8.12",
    "compression": "1.7.3",
    "cors": "^2.8.5",
    "dotenv": "6.1.0",
    "esm": "3.0.84",
    "express": "4.16.3",
    "express-prettify": "0.1.1",
    "express-validator": "5.3.0",
    "helmet": "3.13.0",
    "lodash": "4.17.11",
    "moment": "2.22.2",
    "morgan": "1.9.1",
    "numeral": "2.0.6",
    "pretty-ms": "4.0.0",
    "query-string": "6.2.0",
    "uuid": "3.3.2",
    "web3": "1.0.0-beta.36"
  }
}
